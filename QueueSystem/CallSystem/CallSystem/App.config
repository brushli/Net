<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <connectionStrings>
    <add name="MySQL" connectionString="Database='db_comm';Data Source='cysoft.uicp.net';User Id='root';charset='utf8';pooling=true" />
  </connectionStrings>
  <appSettings>
    <add key="AreaNo" value ="1001"/>
    <add key="IP" value ="cysoft.uicp.net"/>
    <add key="Port" value ="3347"/>
    <!--清屏处理（按时间点进行全部弃号处理，如果有多个时间点中间用逗号分割，时间格式需要严格按照【HH:mm:ss】）-->
    <add key ="ClearTime" value ="12:30:00,18:00:00"/>
    <!--叫号端标识，多个叫号端需要命名不同的名字-->
    <add key ="ClientName" value ="1号叫号端"/>
    <!--窗口区域，分区域叫号，多个区域编号中间用逗号分隔-->
    <add key ="AreaId" value ="1,2"/>
    <!--评价器类型 0：安卓评价器 1 ：4键评价器-->
    <add key ="EvaluatorType" value ="0"/>
  </appSettings>
</configuration>